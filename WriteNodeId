 var requestHeader = new RequestHeader();
 var writeValueCollection = new WriteValueCollection
 {
     new WriteValue() { NodeId = "ns=3;s=\"PLC_PSNEXT_SDB\".\"TEST\"", Value = new DataValue() { Value = true } }
 };
 var statusCodeCollection = new StatusCodeCollection();
 var diagnosticInfoCollection = new DiagnosticInfoCollection();

 _Session.Write(requestHeader, writeValueCollection, out statusCodeCollection, out diagnosticInfoCollection);
